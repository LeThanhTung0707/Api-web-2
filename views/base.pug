doctype html 
html 
    head 
        meta(name='viewport' content="width=device-width, initial-scale=1.0")
        link(href="https://fonts.googleapis.com/css?family=Lato:300,300i,700" rel="stylesheet")
        title Tôi đi mua sắm | #{test}
        link(rel="stylesheet" href="css/style.css")
        link(rel="shortcut icon" type="image/png" href="img/favicon.png" )
        <meta charset="UTF-8" />
    body
        //- HEADER
        header.header
            nav.nav.nav--tours 
                a.nav__el(href="#") All tours 
                form.nav__search
                    button.nav__search-btn 
                        svg 
                            use(xlink:href="img/icons.svg#icon-search")
                    input(type="text" placeholder="Search tours" class="nav__search-input")
            .header__logo
                img(src="img/logo-white.png" alt="Natours logo")
            nav.nav.nav--user 
                //- a.nav__el(href="#") My bookings 
                //- a.nav__el(href="#")
                //-     img.nav__user-img(src="img/users/default.jpg" alt="User photo")
                //-     span ThanhTung 
                button.nav__el Log in 
                button.nav__el.nav__el--cta Sign up
        section.overviews 
            h1= tour
        footer.footer 
            .footer__logo
                img(src="img/logo-green.png" alt="Natours logo")
            ul.footer__nav 
                li: a(href="#") About us
                li: a(href="#") Download apps
                li: a(href="#") Become a guide
                li: a(href="#") Careers
                li: a(href="#") Contact
            p.footer_copyright &copy; by LeThanhTung12a1. All rights reserved.
                
   

                        
    


   
